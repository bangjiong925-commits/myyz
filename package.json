{
  "name": "taiwan-pk10-proxy",
  "version": "1.0.0",
  "type": "module",
  "description": "Taiwan PK10 data proxy and web interface",
  "main": "index.html",
  "scripts": {
    "start": "node proxy-server.js",
    "api": "node api_server_mongodb.js",
    "keys": "node key_management_server.js",
    "analysis": "node pk10-analysis-api.js",
    "dev": "nodemon proxy-server.js",
    "dev-api": "nodemon api_server_mongodb.js",
    "dev-keys": "nodemon key_management_server.js",
    "dev-analysis": "nodemon pk10-analysis-api.js",
    "all": "concurrently \"npm run keys\" \"npm run analysis\""
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.18.2",
    "mongodb": "^6.0.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "html-minifier-terser": "^7.2.0",
    "javascript-obfuscator": "^4.1.1",
    "nodemon": "^3.0.1"
  },
  "license": "MIT"
}
